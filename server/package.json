{
  "name": "ikr-server",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:production": "npm run build --prefix ../client && npm run copyfiles && git add . && git commit -m \"autoupdate\" && git push origin production",
    "copyfiles": "xcopy \"../client/dist\" \"../server/public\" /E /Y",
    "start": "node index.js",
    "start:server": "nodemon index.js --ignore '../client'",
    "start:client": "npm run dev --prefix ../client",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "keywords": ["ikr","Kontenrahmen"],
  "author": "Harald Innetzberger",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
